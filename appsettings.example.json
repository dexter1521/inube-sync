{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "MicrosoftHostingLifetime": "Information"
    },
    "LogsPath": "C:\\Inube\\Logs"
  },
  "AppSettings": {
    "IntervaloMinutos": 2,
    "ApiUrl": "https://tu_sitio_aqui.com/api/",
    "ApiUser": "<tu_usuario_aqui>",
    "DeviceToken": "<tu_token_aqui>",
    "SqlServer": "<tu_cadena_de_conexion_aqui>",
    "TimeoutSeconds": 30,
    "BatchSize": 100,
    "Retry": {
      "MaxRetries": 3,
      "BaseDelaySeconds": 2
    },
    "SubirDatosANube": true
  }
}
